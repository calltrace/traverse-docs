{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/calltrace/traverse/.*"
    },
    {
      "pattern": "^https://marketplace.visualstudio.com/.*"
    },
    {
      "pattern": "^https://docs.rs/traverse-.*"
    },
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://gola.chat"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://calltrace.github.io/traverse-docs/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "text/html",
        "User-Agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 303, 307, 308]
}